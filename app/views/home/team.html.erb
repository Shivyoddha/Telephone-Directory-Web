 .btn-round {

position: relative;
bottom: 10px;

position: fixed;
bottom: 70px;
right: 20px;
border-radius: 50%;
width: 40px;
height: 40px;
text-align: center;
line-height: 40px;
font-weight: bold;
}

@media (max-width: 767px) {
/* Adjust for smaller screens if needed */
.btn-round {
  bottom: 10px;
  right: 10px;
  width: 30px;
  height: 30px;
  line-height: 30px;
}
}

.github-icon{
padding : 0;
text-decoration: none;
font-size: 100px;
}
